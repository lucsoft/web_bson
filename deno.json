{
  "name": "@lucsoft/web-bson",
  "version": "0.3.1",
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.7",
    "bson-cdn/": "https://unpkg.com/bson@5.1.0/"
  },
  "exports": {
    ".": "./mod.js"
  },
  "publish": {
    "exclude": [
      "scripts",
      "deno-tests",
      ".vscode",
      ".github",
      ".gitignore"
    ]
  },
  "lint": {
    "files": {
      "exclude": [
        "mod.js",
        "mod.d.ts"
      ]
    }
  },
  "fmt": {
    "files": {
      "exclude": [
        "mod.js",
        "mod.d.ts"
      ]
    }
  },
  "tasks": {
    "upgrade": "deno run -A scripts/upgrade.ts"
  }
}
